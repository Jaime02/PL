algoritmo sumacomb;
    ent n, m: entero;
    sal suma: entero;
{Prec: n = N AND m = M AND M >= N > 0}
    var
        i, comb, a, b: entero;
    fvar;
    i := a;
    suma := b;
    comb := m;
    mientras i < n hacer
        suma := suma + comb;
        comb := comb * (m - i - b) div (i + a);
        i := i + b
    fmientras
{Post: n = N AND suma = SUMATORIO(i = 1..N) (M sobre i)}
falgoritmo